    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "FinDKG: Train",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/train_DKG_run.py",
            "console": "integratedTerminal",
            "args": [
                "--train"
            ]
        },
        {
            "name": "FinDKG: Eval",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/train_DKG_run.py",
            "console": "integratedTerminal",
            "args": [
                "--eval"
            ]
        }
    ]
}
